;Program: Alternate LED blinking pattern
LEDS EQU P1
ORG 00H
BLINK:
	MOV LEDS, #01010101B	;4 LEDs
	ACALL DELAY
	MOV LEDS, #10101010B	;alternate 4 LEDs
	ACALL DELAY
	AJMP BLINK
	
DELAY:	;Function for delay
		MOV R0, #10
LOOP2:	MOV R1, #200
LOOP1:	MOV R2, #200
LOOP:	DJNZ R2, LOOP
		DJNZ R1, LOOP1
		DJNZ R0, LOOP2
RET
END